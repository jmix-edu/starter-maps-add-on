<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view"
      xmlns:maps="http://jmix.io/schema/maps/ui"
      title="msg://reprojectionSampleView.title">
    <layout>
        <maps:geoMap id="map" width="100%" height="100%">
            <maps:layers>
                <maps:tile>
                    <maps:osmSource/>
                </maps:tile>
                <maps:image>
                    <maps:imageStaticSource url="https://upload.wikimedia.org/wikipedia/commons/thumb/1/18/British_National_Grid.svg/2000px-British_National_Grid.svg.png">
                        <maps:imageExtent minX="0" minY="0" maxX="700000" maxY="1300000"/>
                    </maps:imageStaticSource>
                </maps:image>
            </maps:layers>
        </maps:geoMap>
    </layout>
</view>